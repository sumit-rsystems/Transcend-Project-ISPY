	public static List<TrialPatientXML__c> parseNewCurrentPatient(String xml) {
		
		List<TrialPatientXML__c> lstTrailPatientXML = new List<TrialPatientXML__c>();
		
		Map<String, String> mapTrialPatient = new Map<String, String>();
		List<TrialPatient__c> lstTrialPatient = [select Subject_Id__c from TrialPatient__c];
		for(TrialPatient__c tp : lstTrialPatient) {
			mapTrialPatient.put(tp.Subject_Id__c, tp.Id);
		}
		
		List<TrialPatientXML__c> lstTrialPatientXML = [Select t.TrialPatientId__r.Subject_Id__c, t.Current_Patient_XML__c From TrialPatientXML__c t];
		
		List<XMLDataWrapper> lstTPXMLWrapper = RunCalculationXMLParser.parseXML(xml);
		for(XMLDataWrapper xmlData : lstTPXMLWrapper) {
			boolean flag = false;
			for(TrialPatientXML__c tpXML : lstTrialPatientXML) {
				if(xmlData.patientId == tpXML.TrialPatientId__r.Subject_Id__c) {
					tpXML.Arm__c = xmlData.arm;
					tpXML.Start_Time__c = xmlData.startTime;
					tpXML.HR__c = xmlData.hr;
					tpXML.HER2__c = xmlData.her2;
					tpXML.MP__c = xmlData.mp;
					tpXML.pCR_Time__c = xmlData.pCRTime;
					tpXML.pCR_Value__c = xmlData.pCRValue;
					tpXML.MRI1_Time__c = xmlData.mri1Time;
					tpXML.MRI1_Value__c = xmlData.mri1Value;
					tpXML.MRI2_Time__c = xmlData.mri2Time;
					tpXML.MRI2_Value__c = xmlData.mri2Value;
					tpXML.MRI3_Time__c = xmlData.mri3Time;
					tpXML.MRI3_Value__c = xmlData.mri3Value;
					lstTrailPatientXML.add(tpXML);
					flag = true;
					break;
				}
			}
			if(!flag) {
				if(xmlData.patientId != null) {
					TrialPatientXML__c trialPatientXML = new TrialPatientXML__c();
					trialPatientXML.TrialPatientId__c = mapTrialPatient.get(xmlData.patientId);
					trialPatientXML.Arm__c = xmlData.arm;
					trialPatientXML.Start_Time__c = xmlData.startTime;
					trialPatientXML.HR__c = xmlData.hr;
					trialPatientXML.HER2__c = xmlData.her2;
					trialPatientXML.MP__c = xmlData.mp;
					trialPatientXML.pCR_Time__c = xmlData.pCRTime;
					trialPatientXML.pCR_Value__c = xmlData.pCRValue;
					trialPatientXML.MRI1_Time__c = xmlData.mri1Time;
					trialPatientXML.MRI1_Value__c = xmlData.mri1Value;
					trialPatientXML.MRI2_Time__c = xmlData.mri2Time;
					trialPatientXML.MRI2_Value__c = xmlData.mri2Value;
					trialPatientXML.MRI3_Time__c = xmlData.mri3Time;
					trialPatientXML.MRI3_Value__c = xmlData.mri3Value;
					lstTrailPatientXML.add(trialPatientXML);
				}
			}
		}
		return lstTrailPatientXML;
	}
	
