	public static List<XMLDataWrapper> parseXML(String xml) {
		System.debug('xml from parseXML : '+xml);
		List<XMLDataWrapper> lstTPXML = new List<XMLDataWrapper>();
		XMLDataWrapper tpXML = new XMLDataWrapper();
		Xmlstreamreader reader = new Xmlstreamreader(xml);
		while(reader.hasNext()) {
			if (reader.getEventType() == XmlTag.START_ELEMENT) {
				if (reader.getLocalName() == 'patient' || reader.getLocalName() == 'new_patient') {
					tpXML = new XMLDataWrapper();
					system.debug('Patient Id : '+reader.getAttributeValue(null, 'patient_id'));
					tpXML.patientId = reader.getAttributeValue(null, 'patient_id');
					tpXML.arm = reader.getAttributeValue(null, 'arm');
					tpXML.startTime =  Integer.valueOf(reader.getAttributeValue(null, 'start_time'));
				}
		
				if (reader.getLocalName() == 'patient_covariate') {
					if(reader.getAttributeValue(null, 'name') == 'HR') {
						tpXML.hr = reader.getAttributeValue(null, 'Value') == '1'?true:false;
					}
					if(reader.getAttributeValue(null, 'name') == 'HER2') {
						tpXML.her2 = reader.getAttributeValue(null, 'Value') == '1'?true:false;
					}
					if(reader.getAttributeValue(null, 'name') == 'MP') {
						tpXML.mp = reader.getAttributeValue(null, 'Value') == '1'?true:false;
					}
				}
				
				if (reader.getLocalName() == 'patient_outcome') {
					if(reader.getAttributeValue(null, 'name') == 'pCR') {
						tpXML.pCRTime = Integer.valueOf(reader.getAttributeValue(null, 'time'));
						tpXML.pCRValue = Decimal.valueOf(reader.getAttributeValue(null, 'value'));
					}
					if(reader.getAttributeValue(null, 'name') == 'MRI1') {
						tpXML.mri1Time = Integer.valueOf(reader.getAttributeValue(null, 'time'));
						tpXML.mri1Value = Decimal.valueOf(reader.getAttributeValue(null, 'value'));
					}
					if(reader.getAttributeValue(null, 'name') == 'MRI2') {
						tpXML.mri2Time = Integer.valueOf(reader.getAttributeValue(null, 'time'));
						tpXML.mri2Value = Decimal.valueOf(reader.getAttributeValue(null, 'value'));
					}
					if(reader.getAttributeValue(null, 'name') == 'MRI3') {
						tpXML.mri3Time = Integer.valueOf(reader.getAttributeValue(null, 'time'));
						tpXML.mri3Value = Decimal.valueOf(reader.getAttributeValue(null, 'value'));
					}
				}
			}
			if (reader.getEventType() == XmlTag.END_ELEMENT) {
				if(reader.getLocalName() == 'patient' || reader.getLocalName() == 'new_patient') {
					lstTPXML.add(tpXML);
				}
			}
			reader.next();
		}
		return lstTPXML;
	}
	
