		public Integer pCRTime;
		public Decimal pCRValue;
		public Integer mri1Time;
		public Decimal mri1Value;
		public Integer mri2Time;
		public Decimal mri2Value;
		public Integer mri3Time;
		public Decimal mri3Value;
		
	}
