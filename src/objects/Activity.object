<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionOverrides>
        <actionName>ComposeGmail</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>LogCall</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>MailMerge</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>RequestUpdate</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>SendEmail</actionName>
        <type>Default</type>
    </actionOverrides>
    <actionOverrides>
        <actionName>ViewAll</actionName>
        <type>Default</type>
    </actionOverrides>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>CRF_Form_Id__c</fullName>
        <description>This contains CRF&apos;s records id. Different-Different CRFs like AE_Detail__c record id.</description>
        <externalId>false</externalId>
        <label>CRF Form Id</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>CRF__c</fullName>
        <description>This contains rejected CRF__c object id.</description>
        <externalId>false</externalId>
        <label>CRF</label>
        <length>25</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Group_Id__c</fullName>
        <description>This is used for checking related tasks with a particular group id. Some tasks may be contains same group id.</description>
        <externalId>false</externalId>
        <label>Group Id</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Hours__c</fullName>
        <externalId>false</externalId>
        <label>Hours</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>IsCreateTask__c</fullName>
        <defaultValue>true</defaultValue>
        <description>this field is used for check whether task should be created after completion of this task.</description>
        <externalId>false</externalId>
        <label>IsCreateTask</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>LinkToCRF__c</fullName>
        <externalId>false</externalId>
        <label>Link to CRF</label>
        <required>false</required>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>Minutes__c</fullName>
        <externalId>false</externalId>
        <label>Minutes</label>
        <precision>2</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Object_Label__c</fullName>
        <externalId>false</externalId>
        <label>Object Label</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Object_Name__c</fullName>
        <description>It will contain object API name</description>
        <externalId>false</externalId>
        <label>Object Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Original_Due_Date__c</fullName>
        <description>This is for original task&apos;s due date</description>
        <externalId>false</externalId>
        <label>Original Due Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Proposed_Rescheduling_Id__c</fullName>
        <externalId>false</externalId>
        <label>Proposed Rescheduling Id</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Reschedule_reason__c</fullName>
        <externalId>false</externalId>
        <label>Reschedule_reason</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Schedule_Event_Date__c</fullName>
        <externalId>false</externalId>
        <label>Schedule Event Date</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Trial_Id__c</fullName>
        <description>This trial id will be used in task creation for matching trial on patient detail page, Where we will show the &quot;Next Due Date&quot; of date after match trial id.</description>
        <externalId>false</externalId>
        <label>Trial Id</label>
        <length>25</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Trial__c</fullName>
        <externalId>false</externalId>
        <label>Trial</label>
        <length>100</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>status__c</fullName>
        <externalId>false</externalId>
        <label>status</label>
        <picklist>
            <picklistValues>
                <fullName>Confirmed</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Waiting</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>subject__c</fullName>
        <externalId>false</externalId>
        <label>subject</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <listViews>
        <fullName>MyActivities</fullName>
        <filterScope>Mine</filterScope>
        <label>My Activities</label>
    </listViews>
    <listViews>
        <fullName>MyDelegatedActivities</fullName>
        <filterScope>Delegated</filterScope>
        <label>My Delegated Activities</label>
    </listViews>
    <listViews>
        <fullName>MyTeamsActivities</fullName>
        <filterScope>Team</filterScope>
        <label>My Team&apos;s Activities</label>
    </listViews>
    <listViews>
        <fullName>TodaysTasks</fullName>
        <filterScope>Mine</filterScope>
        <filters>
            <field>TASK.CLOSED</field>
            <operation>equals</operation>
            <value>0</value>
        </filters>
        <filters>
            <field>ACTIVITY.TASK</field>
            <operation>equals</operation>
            <value>1</value>
        </filters>
        <filters>
            <field>TASK.DUE_DATE</field>
            <operation>lessThan</operation>
            <value>TOMORROW</value>
        </filters>
        <label>Today&apos;s Tasks</label>
    </listViews>
    <listViews>
        <fullName>UpcomingEvents</fullName>
        <filterScope>Mine</filterScope>
        <filters>
            <field>ACTIVITY.TASK</field>
            <operation>equals</operation>
            <value>0</value>
        </filters>
        <filters>
            <field>TASK.DUE_DATE</field>
            <operation>greaterThan</operation>
            <value>YESTERDAY</value>
        </filters>
        <label>Upcoming Events</label>
    </listViews>
    <searchLayouts>
        <searchResultsAdditionalFields>TASK.SUBJECT</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>TASK.WHO_NAME</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>TASK.WHAT_NAME</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>TASK.DUE_DATE</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>CORE.USERS.ALIAS</searchResultsAdditionalFields>
        <searchResultsAdditionalFields>Trial__c</searchResultsAdditionalFields>
    </searchLayouts>
</CustomObject>
